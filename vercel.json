{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/unauthorized",
      "destination": "/index.html"
    },
    {
      "source": "/404",
      "destination": "/index.html"
    },
    {
      "source": "(.*)",
      "destination": "/index.html"
    }
  ]
}